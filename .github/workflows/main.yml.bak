name: My Workflow

on:
  workflow_dispatch:
  repository_dispatch:

env:
    SOURCE_URL: https://github.com/sayterdarkwynd/FrackinUniverse
jobs:
  my-job:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: Set up Python 3.10
        uses: actions/setup-python@v2
        with:
            python-version: "3.10"
      - name: Run the script
        run: |
            cd ${{github.workspace}}/script/tools
            python test.py